[run]
source = finance
omit = 
    */migrations/*
    */tests/*
    */venv/*
    */env/*
    */__pycache__/*
    */admin/*
    */static/*
    */templates/*
    manage.py
    */settings.py
    */wsgi.py
    */asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Finance Application Coverage Report

[xml]
output = coverage.xml
